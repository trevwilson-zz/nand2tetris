/**
 * 8-way or gate: out = in[0] or in[1] or ... or in[7].
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=in[0],b=in[1],out=z0);
    Or(a=z0,b=in[2],out=z1);
    Or(a=z1,b=in[3],out=z2);
    Or(a=z2,b=in[4],out=z3);
    Or(a=z3,b=in[5],out=z4);
    Or(a=z4,b=in[6],out=z5);
    Or(a=z5,b=in[7],out=out);
}